{
   "manifest_version": 3,
   "description": "My First  Extension",
   "name": "CTS-Extension",
   "background": {
      "service_worker": "js/background.js"
   },
   "action": {
      "default_title": "CTS-Extension",
      "default_popup": "popup.html"
   },
   "content_scripts": [
      {
         "all_frames": true,
         "css": [
            "style.css"
         ],
         "js": [
            "js/content.js"
         ],
         "matches": [
            "<all_urls>"
         ],
         "run_at": "document_end"
      }
   ],
   "key": "",
   "permissions": [
      "tabs",
      "storage",
      "scripting"
   ],
   "host_permissions": [
      "<all_urls>",
      "https://www.flipkart.com//*",
      "https://www.amazon.in//*"
   ],
   "version": "1.0.0"
}